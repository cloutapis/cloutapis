{
  "name": "cloutapis",
  "version": "1.0.0",
  "description": "A set of useful APIs for BitClout",
  "main": "dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "dev": "nodemon",
    "db:create": "sequelize db:create",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "npm run build && npm start"
  }
}
