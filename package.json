{
  "name": "cloutapis",
  "version": "1.0.0",
  "description": "A set of useful APIs for BitClout",
  "main": "dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "dev": "ts-node-dev --inspect=0.0.0.0:${DEBUG_PORT:-5001} index.ts",
    "dev-worker": "ts-node-dev --inspect=0.0.0.0:${WORKER_DEBUG_PORT:-5002} ./apis/worker.ts",
    "db:create": "sequelize db:create",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "axios": "^0.21.1",
    "bip39": "^3.0.4",
    "body-parser": "^1.19.0",
    "bs58check": "^2.1.2",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.5",
    "elasticsearch": "^16.7.2",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "hdkey": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "jsrsasign": "^10.3.0",
    "jssha": "^3.2.0",
    "key-encoder": "^2.0.3",
    "moment": "^2.29.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  }
}
